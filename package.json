{
  "name": "nextjs-typescript-styled-components-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Mihai Pocorschi <mihai@pocorschi.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pocorschi/nextjs-typescript-styled-components-starter.git"
  },
  "bugs": {
    "url": "https://github.com/pocorschi/nextjs-typescript-styled-components-starter/issues"
  },
  "keywords": [],
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "dev": "next ./src",
    "dev-config": "cross-env DEBUG_CONFIG=true next ./src",
    "build": "next build ./src",
    "build:analyze": "cross-env ANALYZE=true yarn run build",
    "build:maxify": "cross-env ANALYZE=true MINIFY=false yarpm run build",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "start": "next start ./src",
    "test": "jest",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:all": "cross-env DEBUG=eslint:cli-engine eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern '!.eslintrc.js' .",
    "lint-config": "eslint --print-config .eslintrc.js",
    "format": "cross-env DEBUG=eslint:cli-engine eslint --fix --ext .ts,.tsx ./src",
    "format:all": "cross-env DEBUG=eslint:cli-engine eslint --fix --ext .js,.jsx,.ts,.tsx --ignore-pattern '!.eslintrc.js' ."
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6",
    "modern-normalize": "^0.6.0",
    "next": "^9.3.4",
    "next-seo": "^4.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@blunck/next-alias": "^1.0.0",
    "@storybook/react": "^5.3.19",
    "@svgr/webpack": "^5.4.0",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^7.1.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-typescript": "^0.29.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.3",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.1.2",
    "next-size": "^2.0.2",
    "next-transpile-modules": "^3.3.0",
    "node-sass": "^4.12.0",
    "postcss-fixes": "^2.0.1",
    "postcss-preset-env": "^6.7.0",
    "react-is": "^16.13.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "yarpm": "^0.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
