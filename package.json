{
  "name": "nextjs-typescript-styled-components-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Mihai Pocorschi <mihai@pocorschi.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pocorschi/nextjs-typescript-styled-components-starter.git"
  },
  "bugs": {
    "url": "https://github.com/pocorschi/nextjs-typescript-styled-components-starter/issues"
  },
  "keywords": [],
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "dev": "next ./src",
    "dev-config": "cross-env DEBUG_CONFIG=true next ./src",
    "build": "next build ./src",
    "build:analyze": "cross-env ANALYZE=true yarpm run build",
    "build:maxify": "cross-env ANALYZE=true MINIFY=false yarpm run build",
    "start": "next start ./src",
    "test": "jest",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:all": "cross-env DEBUG=eslint:cli-engine eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern '!.eslintrc.js' .",
    "lint-config": "eslint --print-config .eslintrc.js",
    "format": "cross-env DEBUG=eslint:cli-engine eslint --fix --ext .ts,.tsx ./src",
    "format:all": "cross-env DEBUG=eslint:cli-engine eslint --fix --ext .js,.jsx,.ts,.tsx --ignore-pattern '!.eslintrc.js' ."
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6",
    "next": "^9.3.4",
    "next-seo": "^4.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@blunck/next-alias": "^1.0.0",
    "@svgr/webpack": "^4.3.2",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@zeit/next-css": "1.0.2-canary.2",
    "@zeit/next-sass": "1.0.2-canary.2",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.8.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-config-xo-typescript": "^0.15.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.1.2",
    "next-progressbar": "^1.0.0",
    "next-size": "^2.0.2",
    "next-transpile-modules": "^2.3.1",
    "node-sass": "^4.12.0",
    "postcss-fixes": "^2.0.1",
    "postcss-preset-env": "^6.7.0",
    "react-is": "^16.13.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "yarpm": "^0.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
